                        <div id="loginModal" class="modal fade" role="dialog">
                            <div class="modal-dialog">

                                <!-- Modal content-->
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                                        <h4 class="modal-title">Give us your details <span style="font-size: 14px;font-weight: 300;">(Instantly receive bids & best quote from verified vendors that will deliver your weddding on your budget and style)</span></h4>
                                    </div>
                                    <form method="post" id="regForm" name="loginForm" novalidate>
                                        <div class="alert alert-danger alert-dismissable fade in" ng-if="codeError">
                                            <button class="close" style="opacity: 1; color: white" data-dismiss="alert">&times;</button>
                                            <p>
                                                {{codeError}}
                                            </p>
                                        </div>
                                        <div class="alert alert-success alert-dismissable fade in" ng-if="codeMessage">
                                            <button class="close" style="opacity: 1; color: white" data-dismiss="alert">&times;</button>
                                            <p>
                                                {{codeMessage}}
                                            </p>
                                        </div>
                                        <div class="modal-body">
                                            <p>Full Name</p>
                                            <input class="form-control" value="" ng-model="name" ng-disabled="disableInput" id="name" required name="name" type="text">
                                            <span style="font-size: 14px;font-weight: 300; color : red" ng-show="loginForm.name.$dirty && loginForm.name.$invalid">Please enter a valid name</span>
                                            <p>Email</p>
                                            <input class="form-control" value="" ng-model="email" ng-disabled="disableInput" required id="email" name="email" type="email">
                                            <span style="font-size: 14px;font-weight: 300; color : red" ng-show="loginForm.email.$dirty && loginForm.email.$invalid">Please enter a valid email</span>
                                            <p>Password</p>
                                            <input class="form-control" value="" ng-model="password" ng-pattern="/^(?=.*\d)(?=.*[a-z])(?=.*[A-Z])[0-9a-zA-Z]{8,}$/" ng-disabled="disableInput" required id="password" name="password" type="password">
                                            <span style="font-size: 14px;font-weight: 300; color : red" ng-show="loginForm.password.$dirty && loginForm.password.$invalid">Allowed pattern: an uppercase, a number,a lowercase, and at least 8 characters</span>
                                            <p>Phone (Eg:080XXX. A verification code will be sent to this number) </p>
                                            <input class="form-control" value="" id="phone" ng-disabled="vCode" minlength="11" ng-model="phone" required name="phone" type="text">
                                            <span style="font-size: 14px;font-weight: 300;color : red" ng-show="loginForm.phone.$dirty && loginForm.phone.$invalid">Please enter a valid phone number</span>
                                            <p ng-if="vCode">Enter Verification code</p>
                                            <input class="form-control" value="" ng-if="vCode" id="code" name="code" ng-model="verCode" type="text" required>
                                            <span style="font-size: 14px;font-weight: 300; color : red" ng-show="loginForm.code.$dirty && loginForm.code.$invalid">Enter verifiction code</span>
                                            <span style="font-size: 14px;font-weight: 300; " class="pointer" ng-class="(timer) ? 'default' : 'resend'" ng-click="resend();" ng-if="vCode">Edit number and try </span><span ng-show="timer && vCode">in: {{timer}}seconds</span>
                                        </div>
                                        <div class="modal-footer">
                                            <button type="submit" class="button button--rounded" ng-click="save();" ng-disabled="loginForm.$invalid" style="background-color: #7bcad1; color: white" id="save"><i ng-hide="spinner" class="fa fa-spinner fa-2x fa-spin" style="top: 5px;position: relative;"></i>Save</button>
                                            <button type="button" class="button button--rounded" style="background-color: #dde1e5; color: black" data-dismiss="modal">Close</button>
                                        </div>
                                    </form>
                                </div>

                            </div>
                        </div>

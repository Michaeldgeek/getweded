        <!-- end of header -->
        <!-- main section -->
        <div class="tp-page-head">
            <!-- page header -->
            <div class="container">
                <div class="row">
                    <div class="col-md-offset-2 col-md-8">
                        <div class="page-header text-center">
                            <div class="icon-circle">
                                <div class="inner">
                                    <span class="icon"><i class="fi flaticon-two"></i></span>
                                </div>
                            </div>

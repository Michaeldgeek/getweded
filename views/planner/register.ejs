        <div class="tp-breadcrumb">
            <div class="container">
                <div class="row">
                    <div class="col-md-8">
                        <ol class="breadcrumb">
                            <li><a href="#">Home</a></li>
                            <li class="active">Sing Up Vendor</li>
                        </ol>
                    </div>
                </div>
            </div>
        </div>
        <div class="main-container" ng-controller="MainCtrl">
            <div class="container">
                <div class="row">
                    <div class="col-md-7">
                        <div class="well-box">
                            <form name="PlannerForm" novalidate>
                                <h2>Event Planner Registration</h2>
                                <!-- Text input-->
                                <div class="form-group">
                                    <label class="control-label" for="email">E-mail<span class="required">*</span></label>
                                    <input id="email" name="email" placeholder="E-Mail" ng-model="email" class="form-control input-md" required type="email">
                                    <span style="font-size: 14px;font-weight: 300; color : red" ng-show="PlannerForm.email.$dirty && PlannerForm.email.$invalid">Please enter a valid email</span>
                                </div>
                                <div class="form-group">
                                    <label class="control-label" for="name">Name<span class="required">*</span></label>
                                    <input id="name" ng-model="name" name="name" placeholder="Name" class="form-control input-md" minlength="3" required type="text">
                                    <span style="font-size: 14px;font-weight: 300; color : red" ng-show="PlannerForm.name.$dirty && PlannerForm.name.$invalid">Please enter a valid name</span>
                                </div>
                                <div class="form-group">
                                    <label class="control-label" for="phone">Phone<span class="required">*</span></label>
                                    <input id="phone" ng-model="phone" name="phone" placeholder="Phone" class="form-control input-md" required type="text">
                                    <span style="font-size: 14px;font-weight: 300; color : red" ng-show="PlannerForm.phone.$dirty && PlannerForm.phone.$invalid">Please enter a valid phone number</span>
                                </div>
                                <div class="form-group">
                                    <label class="control-label" for="password">Password<span class="required">*</span></label>
                                    <input id="password" ng-model="password" name="password" placeholder="Password" class="form-control input-md" required type="password">
                                    <ul id="strength" check-strength="password"></ul><span id="spanstr">&nbsp;</span>
                                </div>

                                <!-- Button -->
                                <div class="form-group">
                                    <button name="submit" ng-click="registerPlanner(name,email,phone,password);" ng-disabled="PlannerForm.$invalid" class="btn btn-primary btn-lg">Create an account <i ng-hide="!submiting" class="fa fa-spinner fa-spin" ></i></button>
                                </div>
                            </form>
                        </div>
                    </div>
                    <div class="col-md-5">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="feature-block feature-left">
                                    <div class="well-box">
                                        <div class="feature-icon">
                                            <i style="color: #00aeaf" class="fa fa-dashboard fa-3x "></i>
                                        </div>
                                        <div class="feature-content">
                                            <h3>Reach new customers </h3>
                                            <p>Sign up and have access to couples bids...</p>
                                            <a href="#" class="btn btn-default btn-sm">Find out more</a></div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="feature-block feature-left">
                                    <div class="well-box">
                                        <div class="feature-icon">
